{
  "name": "@colch/git-commit-msg",
  "description": "Adds emojis and infers commit context for people who use conventional commits",
  "homepage": "https://github.com/ColCh/git-commit-msg",
  "bugs": "https://github.com/ColCh/git-commit-msg/issues",
  "version": "1.3.1",
  "main": "index.js",
  "license": "MIT",
  "bin": "index.js",
  "files": [
    "index.js"
  ],
  "keywords": [
    "git",
    "commit",
    "hook",
    "emoji",
    "conventional",
    "conventional commits",
    "prepare-commit-msg",
    "commit-msg"
  ],
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "lint": "eslint __tests__/*.js index.js",
    "prettify": "prettier-eslint __tests__/*.js index.js"
  },
  "dependencies": {
    "emoji-suggestions": "1.3.1",
    "emojilib": "2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "husky": ">=1",
    "jest": "24.7.1",
    "lint-staged": ">=8",
    "prettier": "1.17.0",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint",
      "git add"
    ]
  }
}
